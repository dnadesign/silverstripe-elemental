{
    "name": "dnadesign/silverstripe-elemental",
    "description": "Elemental pagetype and collection of Elements",
    "type": "silverstripe-module",
    "keywords": [
        "silverstripe",
        "element",
        "elemental",
        "content blocks"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alexandre Saelens",
            "email": "alexandre.saelens@dna.co.nz"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/cms": "^4.0",
        "silverstripe/versioned": "^1.0",
        "symbiote/silverstripe-gridfieldextensions": "3.0.x-dev",
        "arillo/silverstripe-dataobject-preview": "4.0.x-dev",
        "dnadesign/silverstripe-advanceddropdowns": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "suggest": {
        "bluehousegroup/silverstripe-data-object-version-viewer": "Allows viewing of version history for data objects inside of the SilverStripe CMS",
        "dnadesign/silverstripe-elemental-subsites": "Adds subsite support to elemental",
        "dnadesign/silverstripe-elemental-userforms": "Adds userform support to elemental"
    },
    "extra": {
        "installer-name": "elemental",
        "branch-alias": {
            "dev-ss4": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "DNADesign\\Elemental\\": "src/",
            "DNADesign\\Elemental\\Tests\\": "tests/"
        }
    }
}
